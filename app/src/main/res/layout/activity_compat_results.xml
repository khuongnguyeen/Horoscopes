<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/compat_results_main_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <RelativeLayout
        android:id="@id/compat_results_top_bar"
        android:layout_width="fill_parent"
        android:layout_height="?actionBarSize"
        android:background="@color/titlebar_compat"
        android:gravity="center_vertical">

        <ImageView
            android:id="@id/compat_results_close"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:adjustViewBounds="true"
            android:layout_marginStart="@dimen/_5sdp"
            android:src="@drawable/menu_close"
            android:paddingStart="@dimen/_2sdp"
            android:paddingTop="@dimen/_5sdp"
            android:paddingEnd="@dimen/_2sdp"
            android:paddingBottom="@dimen/_5sdp"
            tools:ignore="ContentDescription" />

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:text="@string/menu_item_22_txt"
            android:textAllCaps="true"
            android:textColor="@android:color/white"
            android:textSize="19sp" />

        <ImageView
            android:id="@id/compat_results_font_size"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:adjustViewBounds="true"
            android:paddingStart="@dimen/_2sdp"
            android:paddingTop="@dimen/_5sdp"
            android:paddingEnd="@dimen/_2sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            android:src="@drawable/font_size"
            tools:ignore="ContentDescription" />
    </RelativeLayout>

    <androidx.core.widget.NestedScrollView
        android:id="@id/compat_results_parent"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="?actionBarSize"
        android:descendantFocusability="beforeDescendants">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@android:color/white">

            <ImageView
                android:id="@id/compat_results_top_img"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:adjustViewBounds="true"
                android:src="@drawable/compat_results_top"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@id/compat_baro"
                android:layout_width="155.0dip"
                android:layout_height="155.0dip"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30.0dip"
                android:src="@drawable/compat_results_baro"
                tools:ignore="ContentDescription" />

            <ProgressBar
                android:id="@id/ring_progress"
                android:layout_width="143.0dip"
                android:layout_height="143.0dip"
                style="?android:progressBarStyleLarge"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="36.0dip" />

            <TextView
                android:id="@id/compat_results_percent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="88.0dip"
                android:alpha="0.85"
                android:text="50%"
                android:textColor="@android:color/white"
                android:textSize="34.0sp"
                tools:ignore="HardcodedText" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginTop="40.0dip"
                android:layout_toStartOf="@id/ring_progress"
                android:gravity="center"
                android:orientation="vertical"
                tools:ignore="UseCompoundDrawables">

                <ImageView
                    android:id="@id/compat_results_sign_left_img"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:adjustViewBounds="true"
                    android:src="@drawable/s1_wh" />

                <TextView
                    android:id="@id/compat_results_sign_left_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/capricorn"
                    android:textAllCaps="true"
                    android:textColor="@android:color/white"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="110.0dip"
                android:layout_toEndOf="@id/ring_progress"
                android:gravity="center"
                android:orientation="vertical"
                tools:ignore="UseCompoundDrawables">

                <ImageView
                    android:id="@id/compat_results_sign_right_img"
                    android:layout_width="38.0dip"
                    android:layout_height="38.0dip"
                    android:adjustViewBounds="true"
                    android:src="@drawable/s1_wh" />

                <TextView
                    android:id="@id/compat_results_sign_right_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/aries"
                    android:textAllCaps="true"
                    android:textColor="@android:color/white"
                    android:textSize="12sp" />
            </LinearLayout>

            <TextView
                android:id="@id/compat_results_top_content"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/compat_results_top_img"
                android:layout_centerHorizontal="true"
                android:paddingStart="@dimen/_15sdp"
                android:paddingTop="@dimen/_2sdp"
                android:text="Your lively nature works wonders, he/she likes puns and a good sense of humor!"
                android:paddingEnd="@dimen/_15sdp"
                android:paddingBottom="@dimen/_10sdp"
                android:textColor="@color/gris"
                android:textSize="18sp"
                tools:ignore="HardcodedText" />

            <LinearLayout
                android:id="@id/compat_results_title_conseil"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/compat_results_top_content"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingTop="5.0dip"
                android:paddingBottom="5.0dip"
                tools:ignore="UseCompoundDrawables">

                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginEnd="@dimen/_8sdp"
                    android:src="@drawable/compat_results_conseil"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:id="@id/compat_results_titltext_conseil"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/compatam_p4_rub1_titre"
                    android:textColor="@color/gris"
                    android:textSize="22sp" />
            </LinearLayout>

            <TextView
                android:id="@id/compat_results_text_conseil"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/compat_results_title_conseil"
                android:layout_centerHorizontal="true"
                android:paddingStart="@dimen/_15sdp"
                android:paddingTop="@dimen/_2sdp"
                android:paddingEnd="@dimen/_15sdp"
                android:paddingBottom="@dimen/_10sdp"
                android:text="Your lively nature works wonders, he/she likes puns and a good sense of humor!"
                android:textColor="@color/gris"
                android:textSize="18sp"
                tools:ignore="HardcodedText"  />

            <LinearLayout
                android:id="@id/compat_results_title_enbref"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/compat_results_text_conseil"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingTop="@dimen/_3sdp"
                android:paddingBottom="@dimen/_3sdp"
                tools:ignore="UseCompoundDrawables">

                <ImageView
                    android:layout_width="20.0dip"
                    android:layout_height="20.0dip"
                    android:layout_marginEnd="10.0dip"
                    android:src="@drawable/compat_results_enbref"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:id="@id/compat_results_titltext_enbref"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/compatam_p4_rub2_titre"
                    android:textColor="@color/gris"
                    android:textSize="22.0sp" />
            </LinearLayout>

            <TextView
                android:id="@id/compat_results_text_enbref"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/compat_results_title_enbref"
                android:layout_centerHorizontal="true"
                android:paddingStart="@dimen/_15sdp"
                android:paddingTop="@dimen/_2sdp"
                android:text="Your lively nature works wonders, he/she likes puns and a good sense of humor!"
                android:paddingEnd="@dimen/_15sdp"
                android:paddingBottom="@dimen/_10sdp"
                android:textColor="@color/gris"
                android:textSize="18sp"
                tools:ignore="HardcodedText"  />

            <LinearLayout
                android:id="@id/compat_results_points"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/compat_results_text_enbref"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:weightSum="2"
                android:paddingTop="@dimen/_5sdp"
                android:paddingBottom="@dimen/_20sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingRight="@dimen/_5sdp">

                    <LinearLayout
                        android:id="@id/compat_results_title_pt_fort"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        tools:ignore="UseCompoundDrawables">

                        <ImageView
                            android:layout_width="@dimen/_15sdp"
                            android:layout_height="@dimen/_15sdp"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:src="@drawable/compat_results_fort" />

                        <TextView
                            android:id="@id/compat_results_titltext_pt_fort"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLines="1"
                            android:text="@string/compatam_p4_rub3_titre"
                            android:textColor="@color/gris"
                            android:textSize="22sp" />
                    </LinearLayout>

                    <TextView
                        android:id="@id/compat_results_text_pt_fort"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/compat_results_title_pt_fort"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:gravity="center"
                        android:text="@string/aries"
                        android:paddingTop="@dimen/_3sdp"
                        android:textColor="@color/gris"
                        android:textSize="18.0sp" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingRight="@dimen/_5sdp">

                    <LinearLayout
                        android:id="@id/compat_results_title_pt_faible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        tools:ignore="UseCompoundDrawables">

                        <ImageView
                            android:layout_width="@dimen/_15sdp"
                            android:layout_height="@dimen/_15sdp"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:src="@drawable/compat_results_faible" />

                        <TextView
                            android:id="@id/compat_results_titltext_pt_faible"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLines="1"
                            android:text="@string/compatam_p4_rub4_titre"
                            android:textColor="@color/gris"
                            android:textSize="22sp" />
                    </LinearLayout>

                    <TextView
                        android:id="@id/compat_results_text_pt_faible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/compat_results_title_pt_faible"
                        android:text="@string/scorpio"
                        android:paddingTop="5.0dip"
                        android:gravity="center"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:textColor="@color/gris"
                        android:textSize="18sp" />
                </RelativeLayout>
            </LinearLayout>

            <RelativeLayout
                android:id="@id/compat_results_again"
                android:layout_width="@dimen/_200sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_below="@id/compat_results_points"
                android:layout_centerHorizontal="true"
                android:background="@drawable/rounded_button_white"
                android:gravity="center">

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/compatam_p4_btn"
                    android:textAllCaps="true"
                    android:textColor="@android:color/black"
                    android:textSize="21sp" />
            </RelativeLayout>

            <LinearLayout
                android:id="@id/compat_results_redir_chat_layout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/compat_results_again"
                android:orientation="vertical"
                android:paddingTop="@dimen/_15sdp"
                android:paddingBottom="@dimen/_15sdp">

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:src="@drawable/hp_redir_chat" />

                <TextView
                    android:id="@id/compat_results_redir_chat_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/_15sdp"
                    android:paddingTop="@dimen/_2sdp"
                    android:text="let's chat"
                    android:textAllCaps="true"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/_12sdp"
                    tools:ignore="HardcodedText,RtlSymmetry" />

                <TextView
                    android:id="@id/compat_results_redir_chat_text"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/_15sdp"
                    android:paddingTop="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:paddingBottom="@dimen/_10sdp"
                    android:text="Do not keep those confusing thoughts to youself. \nOur best psychics are waiting to answer all your questions."
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/_10sdp"
                    tools:ignore="HardcodedText" />

                <RelativeLayout
                    android:id="@id/compat_results_redir_chat_button"
                    android:layout_width="@dimen/_200sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_gravity="center"
                    android:background="@drawable/rounded_button_blue"
                    android:gravity="center">

                    <TextView
                        android:id="@id/compat_results_redir_chat_button_text"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/a_doubt_a_question"
                        android:textAllCaps="true"
                        android:textColor="@android:color/white"
                        android:textSize="@dimen/_10sdp" />
                </RelativeLayout>
            </LinearLayout>

            <RelativeLayout
                android:id="@id/compat_results_fb_native_layout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/compat_results_redir_chat_layout"
                android:layout_marginTop="5.0dip"
                android:layout_marginBottom="5.0dip" />
        </RelativeLayout>
    </androidx.core.widget.NestedScrollView>

